{
    "profiles": {
        "https": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "swagger",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "https://localhost:4101",
            "sslPort": 4101,
            "useSSL": true
        },
        "http": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "swagger",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "http://localhost:4100",
            "httpPort": 4100
        },
        "Docker": {
            "commandName": "Docker",
            "launchBrowser": true,
            "launchUrl": "https://{ServiceHost}:4101/swagger",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Docker"
            },
            "publishAllPorts": true,
            "useSSL": true
        }
