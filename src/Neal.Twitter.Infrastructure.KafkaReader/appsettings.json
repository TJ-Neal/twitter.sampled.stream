{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.Debug"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "System": "Warning",
                "Microsoft": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithProcessId",
            "WithThread"
        ],
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "Console",
                            "Args": {
                                "outputTemplate": "{Timestamp:HH:mm:ss} [{Level}] {Message}\n{Exception}"
                            }
                        },
                        {
                            "Name": "Debug",
                            "Args": {
                                "outputTemplate": "{Timestamp:HH:mm:ss} [{Level}] {Message}\n{Exception}"
                            }
                        }
                    ]
                }
            }
        ],
        "Properties": {
            "Application": "neal-twitter-kafkareader"
        }
    },
    "WrapperConfiguration": {
        "Enabled": true,
        "Topic": "twitter-V2-sampled-stream",
        "BaseUrl": "http://localhost:4200/api/tweets/",
        "ClientConfig": {
            "AutoOffsetReset": "Earliest",
            "BootstrapServers": "localhost:9001"
        }
    }
}